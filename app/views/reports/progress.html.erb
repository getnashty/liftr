<% if @bench==1 %>
<% @count=@count+1 %>
<div style="float:left;width:350px;margin-bottom:10px;">
<div class="headly">
<h1>Bench Press<div class="maxh1"><%=h @maxbench %> lbs max</div></h1>
</div>
<div class="lifts-list">
<div class="head-item" style="text-align:left;">
Date	
</div>
<div class="head-item">
Weight	
</div>
<div class="head-item">
Up/Down
</div>
<div class="head-item">
% Change
</div>
<% today=Time.now.strftime("%m/%d/%Y") %>
<% @weightchange1=0 
   @weightchange2=0
   sum=0
   sum2=0 %>
<% @lifts.each do |lift| %>

<% if lift.user_id == current_user.id && lift.exercise_id==1	
	@weightchange1=lift.weight %>
<div class="max-item" style="clear:both;text-align:left;">
<%=h lift.created_at.strftime("%m/%d/%Y") %>
</div>
<div class="max-item">
<%=h lift.weight %> lbs	
</div>
<div class="max-item">
	<% if @weightchange2!=0
		sum=@weightchange1-@weightchange2+sum %>	
<% @weightdif=@weightchange1-@weightchange2 %>
<%=h @weightchange1-@weightchange2 %> lbs 
<% end %>
</div>
<div class="max-item">
<% if @weightchange2!=0 %>
<%=h (@weightdif*100)/@weightchange2 %> %
<% sum2=(@weightdif*100)/@weightchange2+sum2 %>	
<% end %>
<% @weightchange2=lift.weight %>
</div>
<% end %>
<% end %>

			<div class="max-item" style="text-align:left;">
			</div>
			<div class="max-item">
			<strong>Total:</strong>	
			</div>
			<div class="max-item">
				<%=h sum %> lbs
			</div>
			<div class="max-item">
				<%=h sum2 %> %
			</div>
		
		
</div>
</div>
<% end %>

